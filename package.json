{
  "name": "hubot-gathererbot",
  "version": "1.3.5",
  "description": "A simple hubot that can look up mtg cards and do other magic related things.",
  "main": "index.js",
  "creators": ["Matthew Pfister", "Ben Lyon"],
  "scripts": {
    "shell": "hubot --adapter shell --disable-httpd",
    "test": "gulp coverage"
  },
  "eslintConfig": {
    "rules": {
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "brace-style": 2,
      "eol-last": 2,
      "no-mixed-spaces-and-tabs": 2,
      "quotes": [
        2,
        "single"
      ],
      "semi": 2,
      "spaced-comment": [
        2,
        "always"
      ],
      "wrap-regex": 2
    }
  },
  "keywords": [
    "hubot",
    "hubot-scripts"
  ],
  "devDependencies": {
    "chai": "^3.4.1",
    "coffee-script": "^1.10.0",
    "fluki": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.0.0",
    "gulp-istanbul": "^0.10.3",
    "gulp-mocha": "^2.2.0",
    "hubot": "^2.16.0",
    "mocha": "^2.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "async": "^1.5.2",
    "lodash": "^4.2.1",
    "mtgsdk": "^0.1.1",
    "scryfall-sdk": "^1.0.1"
  }
}
